{
  "name": "Bossy Tab Manager",
  "version": "0.1",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistant": false
  },
  "permissions": [
    "tabs"
  ],
  "optional_permissions": [],
  "browser_action" : {
    "default_popup": "popup/index.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+L",
        "mac": "Command+Shift+L"
      }
    }
  }
}
